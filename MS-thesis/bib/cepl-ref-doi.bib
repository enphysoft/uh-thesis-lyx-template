@Article{Kale-JoCP-1999:,
  Title                    = {NAMD2: Greater Scalability for Parallel Molecular Dynamics},
  Author                   = {Kale, Laxmikant and Skeel, Robert and Bhandarkar, Milind and Brunner, Robert and Gursoy, Attila and Krawetz, Neal and Phillips, James and Shinozaki, Aritomo and Varadarajan, Krishnan and Schulten, Klaus},
  Journal                  = {Journal of Computational Physics},
  Year                     = {1999},
  Number                   = {1},
  Pages                    = {283--312},
  Volume                   = {151},

  Abstract                 = {Molecular dynamics programs simulate the behavior of biomolecular systems, leading to understanding of their functions. However, the computational complexity of such simulations is enormous. Parallel machines provide the potential to meet this computational challenge. To harness this potential, it is necessary to develop a scalable program. It is also necessary that the program be easily modified by applicationâdomain programmers. The NAMD2 program presented in this paper seeks to provide these desirable features. It uses spatial decomposition combined with force decomposition to enhance scalability. It uses intelligent periodic load balancing, so as to maximally utilize the available compute power. It is modularly organized, and implemented using Charm++, a parallel C++ dialect, so as to enhance its modifiability. It uses a combination of numerical techniques and algorithms to ensure that energy drifts are minimized, ensuring accuracy in long running calculations. NAMD2 uses a portable run-time framework called Converse that also supports interoperability among multiple parallel paradigms. As a result, different components of applications can be written in the most appropriate parallel paradigms. NAMD2 runs on most parallel machines including workstation clusters and has yielded speedups in excess of 180 on 220 processors. This paper also describes the performance obtained on some benchmark applications.},
  File                     = {:JoCP-KaleSkeelEtAl-1999-NAMD2GreaterScalability.pdf:PDF},
  ISSN                     = {0021-9991},
  Owner                    = {albertsk},
  Timestamp                = {2016.09.22},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0021999199962010}
}

