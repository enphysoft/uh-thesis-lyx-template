  Keywords                 = {Membrane Distillation,Microporous and porous membranes},
  Mendeley-tags            = {Membrane Distillation},
  Owner                    = {albertsk},
  Timestamp                = {2017.06.19},
  Url                      = {http://www.sciencedirect.com/science/article/pii/037673889400289B}
}

